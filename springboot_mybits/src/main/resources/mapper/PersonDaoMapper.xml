<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jn.springboot_mybits.dao.PersonDao">

    <select id="queryPerson" resultType="Person">
        select  *  from person
    </select>

    <select id="updatePerson" resultType="Person">
       update  person set sex = #{sex}  where name =#{name}
    </select>



</mapper>